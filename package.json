{
  "name": "notes-backend",
  "version": "0.0.1",
  "engines": {
    "node": "12.14.0"
  },
  "description": "backend for notes application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "front": "npm start --prefix ../Client",
    "start": "node index.js",
    "start-prod": "cross-env NODE_ENV=production node index.js",
    "start-dev": "concurrently \"npm run start\" \"npm run front\"",
    "build-front-linux": "cd ../Client && npm run-script build && cd ../Server && npm run start-prod",
    "heroku-postbuild": "npm install --prefix ../Client && npm run build --prefix ../Client",
    "analyze": "source-map-explorer 'build/static/js/*'"
  },
  "author": "szymsi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "mongoose-unique-validator": "^2.0.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.5.0",
    "webfontloader": "^1.6.28"
  }
}
