{
  "name": "notes-backend",
  "version": "0.0.1",
  "description": "backend for notes application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-prod": "cross-env NODE_ENV=production node index.js",
    "client": "npm start --prefix Notes-App-Frontend",
    "server": "node index.js",
    "start-dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-front-linux": "rm -rf build ; cd Notes-App-Frontend && npm run-script build && mv build ../ && cd ../ && npm run start-prod",
    "build-front-windows": "del build & cd Notes-App-Frontend && npm run-script build"
  },
  "author": "szymsi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.11",
    "mongoose-unique-validator": "^2.0.3"
  }
}
