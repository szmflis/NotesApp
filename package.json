{
  "name": "notes-backend",
  "version": "0.0.1",
  "description": "backend for notes application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "front": "npm start --prefix front",
    "server": "node index.js",
    "start-prod": "cross-env NODE_ENV=production node index.js",
    "start-dev": "concurrently \"npm run server\" \"npm run front\"",
    "build-front-linux": "cd front && npm run-script build && cd .. && npm run start-prod",
    "heroku-postbuild": "npm install --prefix front && npm run build --prefix front"
  },
  "author": "szymsi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "mongoose-unique-validator": "^2.0.3"
  }
}
